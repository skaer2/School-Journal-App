      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:School_Journal_App_VSProject.pages.AdminPanel"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AddSubjectPage">

    <Grid>
        <TextBox Name="TitleTxtBx" HorizontalAlignment="Left" Height="27" Margin="33,53,0,0" TextWrapping="Wrap" Text="Title" VerticalAlignment="Top" Width="119"/>
