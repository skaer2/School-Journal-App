        <TextBox Name="GroupTxtBx" HorizontalAlignment="Left" Height="27" Margin="33,145,0,0" TextWrapping="Wrap" Text="Group id" VerticalAlignment="Top" Width="119"/>
        <Button Content="Add" HorizontalAlignment="Left" Height="42" Margin="235,244,0,0" VerticalAlignment="Top" Width="119" Click="AddBtnClick"/>
